<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="davidandroidprojecttools.qq986945193.com.davidandroidprojecttools"    android:installLocation="preferExternal">    <!-- android:installLocation属性。其值为：perferExtenal或auto。     perferExtental——意味着此应用程序安装到扩展存储（通常就是SD卡），但是系统不能保证应用肯定会安装到扩展存储。如果扩展存储没有空间或者不可用，那么系统仍然会将应用程序安装到RAM里。     　　　　auto——意味着此应用程序可能安装到扩展存储，但是对安装位置没有特别偏好，将有系统根据自身算法，参考很多因素之后决定将此应用程序安装到哪里。     　　　　internalOnly——意味着此应用程序只能被安装到RAM。     一旦加入android:installLocation，不论其值为什么，用户都可以将应用程序在RAM和SD卡之间自由移动。-->    //访问网络的权限    <uses-permission android:name="android.permission.INTERNET" />    //查看WiFi状态权限    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />    //查看网络状态权限    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />    //利用Zxing实现二维码的生成与扫描功能    <uses-permission android:name="android.permission.VIBRATE" />    <uses-permission android:name="android.permission.CAMERA" />    <uses-feature android:name="android.hardware.camera" />    <uses-feature android:name="android.hardware.camera.autofocus" />    //拨打电话的权限    <uses-permission android:name="android.permission.CALL_PHONE" />    <application        android:name=".app.MyApplication"        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <!--闪屏页splash-->        <activity android:name=".activity.SplashActivity">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <!--主类-->        <activity            android:name=".activity.MainActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--退出app应用的多种实现方式类-->        <activity            android:name=".activity.SystemAppFourMethodActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--引导页介绍-->        <activity            android:name=".activity.GuideActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--ListVie详解的类-->        <activity            android:name=".activity.ListViewIntroduceActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--圆形头像的实现-->        <activity            android:name=".activity.RoundZoomImageViewActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--无限轮播滚动的banner(封装ViewPager)-->        <activity            android:name=".activity.AutoViewPagerBannerActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--仿微信提示消息个数两种实现方式-->        <activity            android:name=".activity.BadgeViewWaterDropActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--Xutils2使用详解-->        <activity            android:name=".activity.XutilsTwoActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--Imageloader,Picasso,Fresco,Glide图片处理-->        <activity            android:name=".activity.PicassoAndImageloaderFrescoActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--利用Zxing实现二维码的生成与扫描功能-->        <activity            android:name=".activity.BarCodeActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--扫描二维码所需要注册的类-->        <activity            android:name=".zxing.activity.CaptureActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--XListView下拉刷新和上拉加载更多-->        <activity            android:name=".activity.XListViewActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--XListView下拉刷新和上拉加载更多-->        <activity            android:name=".activity.DavidXListViewActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--OkHttp封装后网络请求详解及示例-->        <activity            android:name=".activity.OkHttpActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--仿淘宝,百度外卖,饿了么等头条滚动效果-->        <activity            android:name=".activity.UpMarqueeActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--进入界面弹出一张大图-->        <activity            android:name=".activity.StartDialogImageActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--SwipeRefreshLayout的简单使用教程。下拉刷新控件炫酷效果。-->        <activity            android:name=".activity.SwipeRefreshListViewActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--拨打电话号码的详解代码(比较简单)-->        <activity            android:name=".activity.CallphoneActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!-- 显示星星评论数控件-->        <activity            android:name=".activity.StarBarViewActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <meta-data            android:name="UMENG_APPKEY"            android:value="自己的秘钥"></meta-data>    </application></manifest>