<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="davidandroidprojecttools.qq986945193.com.davidandroidprojecttools">    //访问网络的权限    <uses-permission android:name="android.permission.INTERNET" />    //查看WiFi状态权限    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />    //查看网络状态权限    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />    <application        android:name=".app.MyApplication"        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <!--闪屏页splash-->        <activity android:name=".activity.SplashActivity">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <!--主类-->        <activity            android:name=".activity.MainActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--退出app应用的多种实现方式类-->        <activity            android:name=".activity.SystemAppFourMethodActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <!--引导页介绍-->        <activity            android:name=".activity.GuideActivity"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustPan|stateHidden" />        <meta-data            android:name="UMENG_APPKEY"            android:value="自己的秘钥"></meta-data>    </application></manifest>