<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="davidandroidprojecttools.qq986945193.com.davidandroidprojecttools">    //访问网络的权限    <uses-permission android:name="android.permission.INTERNET" />    //查看WiFi状态权限    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />    //查看网络状态权限    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />    <application        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <activity android:name=".activity.MainActivity">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>    </application></manifest>